[all:vars]
remote_machine_username="admin"
remote_machine_password="1234"
ansible_python_interpreter=/usr/bin/python3
ssh_key_filename="ansible_rsa"


[ssh_setup_group]
server-1_passwd ansible_port=2222 ansible_user=root ansible_password=qwer ansible_host=127.0.0.1


[servers]
server-1 ansible_port=2222 ansible_user=admin ansible_host=127.0.0.1

[servers:vars]
ansible_ssh_private_key_file = ~/.ssh/ansible_rsa
ansible_sudo_pass={{remote_machine_password}}
